{
  "name": "@exactly/subgraph",
  "version": "0.0.1",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "homepage": "https://exact.ly",
  "repository": "github:exactly/subgraph",
  "scripts": {
    "build": "graph build",
    "codegen": "graph codegen",
    "lint": "eslint --ext .ts,.js .",
    "auth": "graph auth --studio",
    "deploy": "graph deploy --studio",
    "prepare:mainnet": "cross-env NETWORK=mainnet mustache views/protocol.js subgraph.template.yaml > subgraph.yaml",
    "prepare:optimism": "cross-env NETWORK=optimism mustache views/protocol.js subgraph.template.yaml > subgraph.yaml",
    "prepare:goerli": "cross-env NETWORK=goerli mustache views/protocol.js subgraph.template.yaml > subgraph.yaml",
    "postinstall": "run-s prepare:goerli codegen"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@exactly/protocol": "^0.2.10",
    "@graphprotocol/graph-ts": "^0.30.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@changesets/types": "^5.2.1",
    "@graphprotocol/graph-cli": "^0.49.0",
    "@types/mustache": "^4.2.2",
    "@types/node": "^20.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "mustache": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "path-exists-cli": "^2.0.0",
    "postinstall-postinstall": "^2.1.0",
    "rescript": "^10.1.4",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^5.0.4"
  }
}
