{
  "name": "@exactly-finance/subgraph",
  "version": "0.0.1",
  "scripts": {
    "build": "graph build",
    "codegen": "graph codegen",
    "lint": "eslint --ext .ts,.js .",
    "auth": "graph auth --product hosted-service",
    "deploy": "graph deploy --product hosted-service",
    "prepare:mainnet": "cross-env NETWORK=mainnet mustache views/protocol.js subgraph.template.yaml > subgraph.yaml",
    "prepare:kovan": "cross-env NETWORK=kovan mustache views/protocol.js subgraph.template.yaml > subgraph.yaml",
    "postinstall": "run-s prepare:kovan codegen"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "dependencies": {
    "@graphprotocol/graph-ts": "^0.26.0"
  },
  "devDependencies": {
    "@exactly-finance/protocol": "exactly-finance/protocol#deploy",
    "@graphprotocol/graph-cli": "^0.29.0",
    "@types/mustache": "^4.1.2",
    "@types/node": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "mustache": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "path-exists-cli": "^2.0.0",
    "postinstall-postinstall": "^2.1.0",
    "solidity-coverage": "^0.7.20",
    "ts-node": "^10.7.0",
    "typechain": "^8.0.0",
    "typescript": "^4.6.3"
  }
}
