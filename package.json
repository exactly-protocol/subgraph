{
  "name": "@exactly-protocol/subgraph",
  "version": "0.0.1",
  "scripts": {
    "build": "graph build",
    "codegen": "graph codegen",
    "lint": "eslint --ext .ts,.js .",
    "auth": "graph auth --studio",
    "deploy": "graph deploy --studio",
    "prepare:mainnet": "cross-env NETWORK=mainnet mustache views/protocol.js subgraph.template.yaml > subgraph.yaml",
    "prepare:optimism": "cross-env NETWORK=optimism mustache views/protocol.js subgraph.template.yaml > subgraph.yaml",
    "prepare:goerli": "cross-env NETWORK=goerli mustache views/protocol.js subgraph.template.yaml > subgraph.yaml",
    "postinstall": "run-s prepare:goerli codegen"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@graphprotocol/graph-ts": "^0.29.3"
  },
  "devDependencies": {
    "@exactly-protocol/protocol": "^0.2.9",
    "@graphprotocol/graph-cli": "^0.40.0",
    "@types/mustache": "^4.2.2",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "mustache": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.5.1",
    "path-exists-cli": "^2.0.0",
    "postinstall-postinstall": "^2.1.0",
    "rescript": "^10.1.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.5"
  }
}
